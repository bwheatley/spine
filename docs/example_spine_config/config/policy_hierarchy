[%- SET network = "network/" _ c.getval('c_subnet');
    SET bu = c.getval('c_bu');
    SET ctype = bu _ "/" _ c.getval('c_ctype');
    SET product = ctype _ "/" _ c.getval('c_product');
    SET class = product _ "/" _ c.getval('c_class');
    SET instance = class _ "/" _ c.getval('c_instance');
    SET host = "host/" _ c.getval('c_hostname_f') -%]
[% network %]
[% bu %]
[% ctype %]
[% product %]
[% class %]
[% instance %]
[% host %]
